local function MYHPIIX_fake_script() -- PRISONERS.Click 
	local script = Instance.new('LocalScript', PRISONERS)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local module = require(script.Parent.Parent.Parent.Parent.Parent.Parent.Module)
	local rs = game:GetService("RunService")
	
	local plr = game.Players.LocalPlayer
	local main = plr.PlayerGui:FindFirstChild("main")
	
	local enabled = false
	
	local connections = {}
	
	
	function esp(v)
		if v.Name == "prisoner" then
			local box = Instance.new("BoxHandleAdornment")
			box.Name = "ESP"
			box.Parent = v
			box.Adornee = v
			box.AlwaysOnTop = true
			box.ZIndex = 10
			box.Size = v.Size
			box.Transparency = 0.5
			box.Color3 = Color3.fromRGB(255, 255, 255)
		end
	end
	
	
	script.Parent.Selector.MouseButton1Click:Connect(function()
		if enabled then
			module.getDisableTween(script.Parent.Selector):Play()
			enabled = false
	
			for _, v in workspace.debris:GetChildren() do
				if v.Name == "prisoner" then
					v:FindFirstChild("ESP"):Destroy()
				end
			end
		else
			module.getEnableTween(script.Parent.Selector):Play()
			enabled = true
	
			for _, v in workspace.debris:GetChildren() do
				esp(v)
			end
	
			table.insert(connections, 
				workspace.debris.ChildAdded:Connect(function(v)
					esp(v)
				end)
			)
		end
	end)
end
