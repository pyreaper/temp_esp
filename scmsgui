CategoryText.BorderSizePixel = 0
CategoryText.Position = UDim2.new(0.269990087, 0, 0.0166879501, 0)
CategoryText.Size = UDim2.new(0.699715555, 0, 0.0755784735, 0)
CategoryText.Font = Enum.Font.Cartoon
CategoryText.Text = "Select an category"
CategoryText.TextColor3 = Color3.fromRGB(255, 255, 255)
CategoryText.TextScaled = true
CategoryText.TextSize = 37.000
CategoryText.TextWrapped = true

UIAspectRatioConstraint.Parent = Main
UIAspectRatioConstraint.AspectRatio = 1.263

-- Module Scripts:

local fake_module_scripts = {}

do -- _9HUH9.Module
	local script = Instance.new('ModuleScript', _9HUH9)
	script.Name = "Module"
	local function module_script()
		local module = {}
		
		local ts = game:GetService("TweenService")
		local info = TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut)
		
		function module.getEnableTween(selector) 
			return ts:Create(
				selector.Icon.Enabled,
				info,
				{ImageTransparency=0}
			)
		end
		
		function module.getDisableTween(selector) 
			return ts:Create(
				selector.Icon.Enabled,
				info,
				{ImageTransparency=1}
			)
		end
		
		
		
		return module
		
	end
	fake_module_scripts[script] = module_script
end


-- Scripts:

local function VKPIT_fake_script() -- _9HUH9_2.LocalScript 
	local script = Instance.new('LocalScript', _9HUH9_2)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Main.Visible = not script.Parent.Parent.Main.Visible
	end)
end
coroutine.wrap(VKPIT_fake_script)()
local function XOABRSB_fake_script() -- VT.Click 
	local script = Instance.new('LocalScript', VT)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local module = require(script.Parent.Parent.Parent.Parent.Parent.Parent.Module)
	local rs = game:GetService("RunService")
	
	local plr = game.Players.LocalPlayer
	local main = plr.PlayerGui:FindFirstChild("main")
	
	local enabled = false
	
	local connections = {}
	
	script.Parent.Selector.MouseButton1Click:Connect(function()
		if enabled then
			module.getDisableTween(script.Parent.Selector):Play()
			enabled = false
			
			game.Workspace:FindFirstChildOfClass("Atmosphere").Parent = game.Lighting
			main.dmg.Visible = true
			
			for _, v in pairs(connections) do
				v:Disconnect()
			end
		else
			module.getEnableTween(script.Parent.Selector):Play()
			enabled = true
			
			game.Lighting:FindFirstChildOfClass("Atmosphere").Parent = game.Workspace
			main.dmg.Visible = false
			
			table.insert(connections, 
				plr.CharacterAdded:Connect(function(char)
					main.dmg.Visible = false
				end)
			)
			
			table.insert(connections, 
				rs.Heartbeat:Connect(function()
					game.Lighting.ClockTime = 14
				end)
				
			)
			
			table.insert(connections,
				rs.RenderStepped:Connect(function()
					game.Lighting.Ambient = Color3.fromRGB(138, 134, 114)
					game.Lighting.OutdoorAmbient = Color3.fromRGB(128, 127, 110)
				end)
			)
		end
	end)
end
coroutine.wrap(XOABRSB_fake_script)()
local function WSLI_fake_script() -- AM.Click 
	local script = Instance.new('LocalScript', AM)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local module = require(script.Parent.Parent.Parent.Parent.Parent.Parent.Module)
	local uis = game:GetService("UserInputService")
	
	local plr = game.Players.LocalPlayer
	
	local enabled = false
	
	script.Parent.Selector.MouseButton1Click:Connect(function()
		if enabled then
			module.getDisableTween(script.Parent.Selector):Play()
			enabled = false
			mouse1release()
		else
			module.getEnableTween(script.Parent.Selector):Play()
			enabled = true
			mouse1press()
		end
	end)
	
	uis.InputBegan:Connect(function(input, _g)
		if _g then return end
		if input.KeyCode == Enum.KeyCode.T then
			if enabled then
				mouse1release()
				module.getDisableTween(script.Parent.Selector):Play()
				enabled = false
			else
				module.getEnableTween(script.Parent.Selector):Play()
				enabled = true
				mouse1press()
			end
		end
	end)
end
coroutine.wrap(WSLI_fake_script)()
local function FNHSFPW_fake_script() -- PLAYERS.Click 
	local script = Instance.new('LocalScript', PLAYERS)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local module = require(script.Parent.Parent.Parent.Parent.Parent.Parent.Module)
	local rs = game:GetService("RunService")
	
	local plr = game.Players.LocalPlayer
	local main = plr.PlayerGui:FindFirstChild("main")
	
	local enabled = false
	
	local connections = {}
	
	function esp(char)
		if plr.Character == char then return end
		local humanoid = char:FindFirstChildOfClass("Humanoid")
		humanoid.NameDisplayDistance = 100
	
		for _, v in char:GetChildren() do
			if v:IsA("BasePart") then
				local box = Instance.new("BoxHandleAdornment")
				box.Name = v.Name.." ESP"
				box.Parent = char
				box.Adornee = v
				box.AlwaysOnTop = true
				box.ZIndex = 10
				box.Size = v.Size
				box.Transparency = 0.5
				box.Color3 = Color3.fromRGB(255, 255, 255)
			end
		end
	end
	
	
	
	script.Parent.Selector.MouseButton1Click:Connect(function()
		if enabled then
			module.getDisableTween(script.Parent.Selector):Play()
			enabled = false
			
			for _, v in pairs(connections) do
				v:Disconnect()
			end
			
			for _, v in workspace.players:GetChildren() do
				for _, b in v:GetChildren() do
					print(string.find(b.Name, " ESP"))
					if string.find(b.Name, " ESP") then
						b:Destroy()
					end
				end
			end
		else
			module.getEnableTween(script.Parent.Selector):Play()
			enabled = true
			
			for _, v in workspace.players:GetChildren() do
				esp(v)
			end
			
			table.insert(connections, 
				workspace.players.ChildAdded:Connect(function(char)
					esp(char)
				end)
			)
		end
	end)
end
coroutine.wrap(FNHSFPW_fake_script)()
