	local script = Instance.new('LocalScript', PILLAR)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local module = require(script.Parent.Parent.Parent.Parent.Parent.Parent.Module)
	
	local plr = game.Players.LocalPlayer
	local main = plr.PlayerGui:FindFirstChild("main")
	
	local enabled = false
	
	script.Parent.Selector.MouseButton1Click:Connect(function()
		if enabled then
			module.getDisableTween(script.Parent.Selector):Play()
			enabled = false
			
			for _, v in workspace.map.rocks:GetChildren() do
				if v.Name == "pillar" then
					v:FindFirstChild("ESP"):Destroy()
				end
			end
		else
			module.getEnableTween(script.Parent.Selector):Play()
			enabled = true
			
